wcnt-1.25/jwmsynth

header header.wch

synth_file_reader
timefile
    filename time.wch
    mod_action  exclude
    dobj_action exclude
    modules
    modules
    data_objects
    data_objects
timefile

sync_clock
clock1
    in_bpm            time1 out_bpm
    in_pos_step_size  time1 out_pos_step_size
    in_beats_per_bar  time1 out_beats_per_bar
    in_beat_value     time1 out_beat_value
    in_phase_trig     time1 out_bar_trig
    in_freq_mod1      off
    in_freq_mod2      off
    freq_mod1_size  1.0
    freq_mod2_size  1.0
    quarter_value   2
    len             1
    snap_to         1
clock1

stepper
stepper1
    step_shape
        stepvx  pos 1.00    ul 0.0   ll 0.00
        stepvx  pos 0.75    ul 0.25  ll 0.25
        stepvx  pos 0.50    ul 0.50  ll 0.50
        stepvx  pos 0.25    ul 0.75  ll 0.75
        stepvx  pos 0.00    ul 1.0   ll 1.00
    step_shape
    in_trig          clock1 out_phase_trig
    in_restart_trig  time1  out_bar_trig
    in_modulation    off
    step_count      16
    up_thresh       0.95
    lo_thresh       0.25
    response_time   0.0
    recycle_mode    on
stepper1


wavfilein
drloop
    filename    samples/drumloop01-2b.wav
    root_note   c0
drloop

clock
clock-s1f
    in_freq_mod1    adsr1   out_output
    freq            240.0
    freq_mod1_size  1.21
clock-s1f

sampler
sampler1
        in_play_trig        clock1      out_phase_trig
        in_stop_trig        off
        in_start_pos_mod    stepper1    out_output
        in_deg_size         clock-s1f  out_deg_size
        wavfile                 drloop
        play_dir                fwd
        play_mode               wrap
        jump_mode               play
        start_pos_min           0
        start_pos_max           88221
        loop_mode               off
        loop_start_pos          3500
        loop_end_pos            5000
        loop_is_offset          on
        bi-directional_offset   0
        anti_clip_samples       5
        anti_clip_each_end      off
        zero_search_range       0
        degsize_amount          1.0
sampler1

pattern_trig
pt1
    in_trig clock1  out_phase_trig
    pattern 1111-1110-1011-1101
pt1

adsr
adsr1
        envelope
        section name attack
            ut  1    ul  0.75
            lt  1    ll  0.75
        section name attack
            ut  2    ul  1.0
            lt  2    ll  1.0
        section name decay
            ut  50    ul  0.94
            lt  50    ll  0.94
        section name release
            ut  150    ul  0.65
            lt  150    ll  0.65
        section name release
            ut  75    ul  0.0
            lt  75    ll  0.0
        envelope
        in_note_on_trig     pt1  out_trig
        in_note_off_trig    off
        in_velocity         off
        up_thresh        1.0
        lo_thresh        0.0
        start_level      0.0
        min_time         0.0
        max_sustain_time 0.0
        release_is_ratio on
        sustain_state    off
        zero_retrigger   off
adsr1

copier c1 from clock1 to clock2 c1

stepper
stepper2
    step_shape
        stepvx  pos 0.00    ul 0.0   ll 0.00
        stepvx  pos 0.25    ul 0.25  ll 0.25
        stepvx  pos 0.50    ul 0.50  ll 0.50
        stepvx  pos 0.75    ul 0.75  ll 0.75
        stepvx  pos 1.00    ul 1.0   ll 1.00
    step_shape
    in_trig          clock2 out_phase_trig
    in_restart_trig  time1  out_bar_trig
    in_modulation    off
    step_count      256
    up_thresh       0.95
    lo_thresh       0.25
    response_time   0.0
    recycle_mode    on
stepper2

pattern_trig
pt2
    in_trig clock2  out_phase_trig
    pattern 1000-0000-0000-0000-0000-0000-0000-0000
pt2

clock
clock-s2f
    in_freq_mod1    adsr1   out_output
    freq            290.0
    freq_mod1_size  2.0
clock-s2f

copier c3 from sampler1 to sampler2 c3
copier c4 from adsr1 to adsr2 c4

param_editor
pe1
    editlist
        edit clock2
            quarter_value 64.0
        edit sampler2
            loop_mode bi
            degsize_amount 1.0
            anti_clip_samples 125
    editlist
pe1

input_editor
ie1
    editlist
        connect sampler2
            in_deg_size         clock-s2f   out_deg_size
            in_play_trig        clock2      out_phase_trig
            in_start_pos_mod    stepper2    out_output
        connect adsr2
            in_note_on_trig     pt2         out_trig
    editlist
ie1

multiplier m1 in_signal sampler1 out_l in_modifier adsr1 out_output m1
multiplier m2 in_signal sampler2 out_l in_modifier adsr2 out_output m2

dynamic
dyn-m1
    amp_map
        dvertex
            sigin_level 0.0
                so_ul 3.0
                so_ll 1.0
        dvertex
             sigin_level 0.25
                so_ul 2
                so_ll 1.0
        dvertex
             sigin_level 0.5
                so_ul 1.5
                so_ll 1.0
        dvertex
             sigin_level 0.75
                so_ul 1.25
                so_ll 1.0
        dvertex
             sigin_level 1.0
                so_ul 1.0
                so_ll 1.0
    amp_map
    in_signal       m1      out_output
    in_modulation   adsr1   out_output
    up_thresh       0.5
    lo_thresh       0.0
    posneg_mirror   on
    use_ratios      on
dyn-m1

clock
clock3
    in_freq_mod1    off
    freq            1.0
    freq_mod1_size  1.0
clock3

sine_wave
sine1
    in_phase_trig   clock3  out_phase_trig
    in_deg_size     clock3  out_deg_size
    recycle_mode         on
    on_trig_reset_phase  off
    phase_cycles         1.0
sine1

invert sine2 in_signal sine1 out_output sine2

panner
pan1
    in_signal   dyn-m1  out_output
    in_pan_mod  sine1   out_output
    pan           0.1
    pan_mod_size  0.165
pan1

panner
pan2
    in_signal   m2      out_output
    in_pan_mod  sine2   out_output
    pan           -0.1
    pan_mod_size  0.165
pan2

stereo_amp
amp1
    in_l        pan1    out_l
    in_r        pan1    out_r
    in_amp_eg   one     out_output
    in_amp_mod  off
    left_amplitude     32767
    right_amplitude    32767
    amp_mod_size  0.0
    clip_level    32767
amp1

stereo_amp
amp2
    in_l        pan2    out_l
    in_r        pan2    out_r
    in_amp_eg   one     out_output
    in_amp_mod  off
    left_amplitude     22000
    right_amplitude    22000
    amp_mod_size  0.0
    clip_level    32767
amp2

mix_chan
ch1
    in_left     amp1    out_left
    in_right    amp1    out_right
ch1

mix_chan
ch2
    in_left     amp2    out_left
    in_right    amp2    out_right
ch2

mixer
mix1
    mix
        synthmod name  ch1
        synthmod name  ch2
    mix
    master_level  1
mix1


wavfile_out
wav1
    in_left     mix1    out_left
    in_right    mix1    out_right
    in_bar      time1   out_bar
    in_bar_trig time1   out_bar_trig
    filename    tester.wav
    start_bar   0
    end_bar     32
wav1

wcnt_exit
wcnt_exit_module
    in_bar  time1   out_bar
    exit_bar    32
wcnt_exit_module

