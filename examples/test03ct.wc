                        	so_ll   1.25
                dvertex
                        sigin_level     1.0
                        	so_ul   1.0
                        	so_ll   1.0
        amp_map

        in_signal               lpf1l out_wet_output
        in_modulation           off

        up_thresh               1.0
        lo_thresh               0.0
        posneg_mirror           on
	use_ratios		on
lpfd1l

