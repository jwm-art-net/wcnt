// wcnt.sf.net
//
//-------------------------------------

header header.wch

// read and create totallity of sinewave.wc

synth_file_reader
sinwvfile
    filename    sinewave.wc
    mod_action  exclude // use include with an empty list
    dobj_action exclude // to include nothing.
    modules
        mod name amp1 // exclude amp1
    modules
    data_objects
        // empty list 2
    data_objects
sinwvfile

peak_detector
pkdt1
    in_signal       sinewave    out_output
    sig_range_hi    0.99999
    sig_range_lo    -0.99999
    message       wcnt_-_Wav_Composer_Not_Toilet-1.25_is_now_released!
    force_abort   off
    max_peaks     5
pkdt1

square_wave
squarewave
    in_phase_trig  clock1 out_phase_trig
    in_deg_size    clock1 out_deg_size
    in_pwm         sine2    out_output
    pulse_width   0.5
    pwm_size      0.9125
    recycle_mode  off
squarewave

clock
clock2
    in_freq_mod1    off
    freq            0.095
    freq_mod1_size  1.0
clock2

sine_wave
sine2
    in_phase_trig  clock2 out_phase_trig
    in_deg_size    clock2 out_deg_size
    recycle_mode        on
    on_trig_reset_phase off
    phase_cycles        1.0
sine2

invert
isq
    in_signal squarewave    out_output
isq

multiplier mm1
    in_signal   sinewave  out_output
    in_modifier isq       out_output
mm1

constant x value 0.6 x // pitch change amount kinda

noise_generator ng1 ng1

modifier
mm1x
    in_signal    mm1 out_output
    in_modifier  ng1 out_output
    function  add
    bias      0.5
mm1x

//adder
//mm1x
//    in_signal mm1   out_output
//    in_modifier x   out_output
//mm1x

copier clkcp1 from clock2 to panclk clkcp1
copier sncp1 from sine2 to pansin sncp1

panner
pan1
    in_signal   sinewave    out_output
    in_pan_mod  pansin      out_output
    pan           0.0
    pan_mod_size  0.99
   
pan1

echo
echosl
    in_signal        pan1   out_l
    in_gain_mod      off
    in_feedback      echosr  out_wet_output
    in_feedback_mod  off
    delay_time        125
    gain              1.0
    gain_modsize      0.0
    feedback          -0.5
    feedback_modsize  0.0
    wet/dry           0.5
echosl

echo
echosr
    in_signal        pan1   out_r
    in_gain_mod      off
    in_feedback      echosl  out_wet_output
    in_feedback_mod  off
    delay_time        155
    gain              1.0
    gain_modsize      0.0
    feedback          0.5
    feedback_modsize  0.0
    wet/dry           0.5
echosr

stereo_amp
stamp
    in_l        echosl out_output
    in_r        echosr out_output
    in_amp_eg   one    out_output
    in_amp_mod  off
    left_amplitude   26767
    right_amplitude  26767
    amp_mod_size     0.0
    clip_level       32767
stamp

// edit inputs and params here, before group copying, so as
// to not have to do so with each group copy.

input_editor
inputs1
    editlist
        connect clock1 in_freq_mod1 mm1x out_output
        connect pansin 
            in_phase_trig panclk out_phase_trig
            in_deg_size panclk out_deg_size
        connect wav1
            in_left     mix1    out_left
            in_right    mix1    out_right
    editlist
inputs1

param_editor
params11
    editlist
        edit clock1
            freq_mod1_size 1.25 // change me
        edit wav1
            filename        wcnt-1.25.wav
    editlist
params11

mix_chan ch_out in_left stamp out_left in_right stamp out_right ch_out

group
g1
    modules
        synthmod name clock1
        synthmod name sinewave
        synthmod name squarewave
        synthmod name mm1
        synthmod name mm1x
        synthmod name isq
        synthmod name pkdt1
        synthmod name echosl
        synthmod name echosr
        synthmod name panclk
        synthmod name pansin
        synthmod name pan1
        synthmod name stamp
        synthmod name ch_out
    modules
g1

copier grpcp1 from g1 to g2  grpcp1
copier grpcp2 from g1 to g3  grpcp2
copier grpcp3 from g1 to g4  grpcp3
copier grpcp4 from g1 to g5  grpcp4
copier grpcp5 from g1 to g6  grpcp5
copier grpcp6 from g1 to g7  grpcp6
copier grpcp7 from g1 to g8  grpcp7
copier grpcp8 from g1 to g9  grpcp8
copier grpcp9 from g1 to g10 grpcp9

param_editor
