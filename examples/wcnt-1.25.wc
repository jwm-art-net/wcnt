    editlist
params1

input_editor
inpsfb
    editlist
        connect g1.mm1  in_modifier g2.isq  out_output
        connect g2.mm1  in_modifier g3.isq  out_output
        connect g3.mm1  in_modifier g4.isq  out_output
        connect g4.mm1  in_modifier g5.isq  out_output
        connect g5.mm1  in_modifier g6.isq  out_output
