header header.wch

synth_file_reader
cratty01_wc
    filename cratty01.wc
    mod_action  exclude // exclude all modules specified below
    dobj_action exclude // exclude all data_objects specified below
    modules
        mod name adsr1
    modules
    data_objects
    data_objects
cratty01_wc

adsr
adsr1
        envelope
        section name attack
            ut  100    ul  0.75
            lt  100    ll  0.75
        section name attack
            ut  25    ul  1.0
            lt  25    ll  1.0
        section name decay
            ut  135    ul  0.6
            lt  135    ll  0.6
        section name release
            ut  155    ul  0.75
            lt  155    ll  0.75
        section name release
            ut  325    ul  0.0
            lt  325    ll  0.0
        envelope
        in_note_on_trig     seq1 out_note_on_trig
        in_note_off_trig    seq1 out_note_off_trig
        in_velocity         seq1 out_velocity
        up_thresh        1.0
        lo_thresh        0.0
        start_level      0.0
        min_time         0.0
        max_sustain_time 0.0
        release_is_ratio on
        sustain_state    on
        zero_retrigger   off
adsr1

riff_editor
re2a
    riff riff1
    notelist
        note name >P*P  pos 0.0 len 2.0 vel 0.0
    notelist
re2a

riff_editor
re2b
    riff riff2
    notelist
        note name >P*P  pos 0.0 len 2.0 vel 0.0
    notelist
re2b

copier oc2  from osc1 to osc1low oc2
trigger
lvltrig
    in_signal       lpfL    out_output
    delay_time      1000.0
    trigger_level   0.75
lvltrig

adsr
adsr2
