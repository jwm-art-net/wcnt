param_editor
cratty_pe1
    editlist
        edit osc1
            octave_offset 0
        edit osc2
            octave_offset 0
            tuning_semitones -6
        edit osc1low
            octave_offset -6
        edit clock1
            freq        75.0
            freq_mod1_size 1.0
        edit pt2
            pattern 11111110-11111110-11111110-11111000
        edit stepper1
            step_count  350
        edit sampler1
            anti_clip_samples   150
        edit sampler2
            anti_clip_samples   150
        edit boost
            value 1.0
        edit echo1
            delay_time  750
            wet/dry     0.75
            feedback    0.75
        edit echo2
            delay_time  675
            wet/dry     0.75
            feedback    0.75
        edit lpfL
            feedback_modsize 1.0
        edit lpfR
            feedback_modsize 1.0
        edit wav1
            filename    cratty02.wav
            end_bar     38
        edit wcnt_exit_module
            exit_bar    38
    editlist
cratty_pe1

input_editor
cratty_ie1
    editlist
        connect stepper1
            in_trig         osc1low out_phase_trig
        connect lpfL
            in_feedback     echo2   out_output
            in_feedback_mod adsr2   out_output
        connect lpfR
            in_feedback     echo1   out_output
            in_feedback_mod adsr2   out_output
        connect echo1
            in_signal       lpfL    out_output
        connect echo2
            in_signal       lpfR    out_output
        connect amp1
            in_amp_eg       adsr3   out_output
    editlist
cratty_ie1

