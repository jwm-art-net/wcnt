wcnt-1.30/jwmsynth

samplerate 44100

timer
t-on
    timings
        time seconds 0.0
        time seconds 6.0
    timings
    time_is_relative off
    recycle_mode    off
t-on

timer
t-off
    timings
        time seconds 5.0
    timings
    time_is_relative off
    recycle_mode    off
t-off

adsr env
    envelope
        section name attack
            ut  50  ul  1
            lt  50  ll  1
        section name decay
            ut  50  ul  0.75
            lt  50  ll  0.75
        section name release
            ut  150 ul  0.0
            lt  150 ll  0.0
    envelope
    in_note_on_trig     t-on    out_trig
    in_velocity         off
    sustain_state       on
    in_note_off_trig    t-off   out_trig
env

clock
cc
    freq 16
cc

inspector
i
    _in_float_          env out_output
    _float_msg_         env:out_output=
    _in_float_trig_     cc out_phase_trig

    _in_int_            off
    _int_msg_           .
    _in_int_trig_       off

    _in_sample_         off
    _sample_msg_        .
    _in_sample_trig_    off

    _in_state_          off
    _state_msg_         .
    _in_state_trig_     off

    _in_string_         off
    _string_msg_        .
    _in_string_trig_    off
i

wavfile_out
wav
    filename       adsr_output.wav
    data_format    pcm16
    in_left      env    out_output
    in_right     env    out_output
    in_bar       t-on   out_count
    start_bar      0
    end_bar        1
    snapshot_mode  off
wav

wcnt_exit
exit
    in_bar      t-on out_count
    exit_bar    1
exit

