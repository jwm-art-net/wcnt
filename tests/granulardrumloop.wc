wcnt-1.30/jwmsynth

samplerate 44100

timer
t-on
    timings
        time seconds 0.0
        time seconds 30.0
    timings
    time_is_relative off
t-on

lfo_clock
lfoc
    freq   24.0
    in_phase_trig off
    in_freq_mod1 w1 out_output
    freq_mod1_size 4.0
lfoc

constant_freq f1
    freq 0.14
f1

wave w1 type sine in_phase_step f1 out_phase_step w1

wavfilein
crappytv
    filename    ../examples/samples/drumloop01-2b.wav
    root_note   c0
crappytv

constant_note
n1
    name d0
n1

sampler
s1
    wavfile             crappytv
    in_play_trig        lfoc out_phase_trig
    in_phase_step       n1 out_phase_step
    in_start_pos_mod    w1 out_output
    start_pos_min       0
    start_pos_max       40000
    anti_clip_samples   150
    anti_clip_each_end  on
    zero_search_range   0
s1

wavfile_out
file
    filename    granulardrumloop_output.wav
    data_format pcm16
    in_left     s1 out_left
    in_right    s1 out_right
    in_bar      t-on out_count
    start_bar   0
    end_bar     1
file

wcnt_exit
exit
    in_bar      t-on out_count
    exit_bar    1
exit

wcnt-1.30/jwmsynth



