wcnt-1.30/jwmsynth


samplerate 44100

constant one value 1.0 one

// duration of sound

timer
t
    timings
        time seconds 0.0
        time seconds 0.125
        time seconds 0.125
        time seconds 0.125
        time seconds 0.5
        time seconds 0.85
    timings
    time_is_relative on
    recycle_mode on
t

adsr
eg
    envelope
        section name   attack   time 2.5    level 1.0
        section name   decay    time 50    level 0.5
        section name   release  time 150.0    level 0.0
    envelope
    in_note_on_trig     t out_trig
    in_note_off_trig    off
    sustain_state       off
eg

noise_generator ng ng


multiplier noise in_signal1 ng out_output in_signal2 eg out_output noise

caps_plate
revb
    in_left    noise out_output
    bandwidth  0.999
    tail       0.999
    damping    0.0005
    wet/dry    0.125
revb


wavfile_out
wav1
    in_left     revb out_left
    in_right    revb out_right
    in_bar      t   out_count
    data_format float32
    filename    reverb.wav
    start_bar   0
    end_bar     25
wav1


wcnt_exit
wcnt_exit_module
    in_bar      t out_count
    exit_bar    25
wcnt_exit_module



