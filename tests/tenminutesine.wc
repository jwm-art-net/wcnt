wcnt-1.30/jwmsynth

samplerate 44100

; simple.wc
;   processes ten minutes worth of a sine wave with fade in and fade out
;   at end. does not write to disk. used for timing purposes.
;

timer
t
    timings
        time seconds 0
        time seconds 6000
    timings
    time_is_relative off
    recycle_mode off
t

clock
c
    in_freq_mod1 off
    freq 220
    freq_mod1_size 1.0
c

wave
s
    in_phase_step   c out_phase_step
    type            sine
s

fader
f
    in_bar      t out_count
    start_bar      0
    end_bar        1
    fade_in_time   10
    fade_out_time  30
f

multiplier
m
    in_signal1 s out_output
    in_signal2 f out_output
m

wcnt_exit
x
    in_bar      f out_index
    exit_bar    1
x

wcnt-1.30/jwmsynth

wavfile_out
w
    in_left         m out_output
    in_right        m out_output
    in_bar          f out_index
    snapshot_mode   off
    data_format     float32
    filename        tenminutesine.wav
    start_bar       0
    end_bar         1
w
