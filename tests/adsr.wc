wcnt-1.26/jwmsynth

samplerate 44100

timer
t
    timings
        time seconds 0.0
        time seconds 1.0
    timings
t

clock
c
    in_freq_mod1 off
    freq 1
    freq_mod1_size 1
c

pattern_trig
pt
    in_trig c out_phase_trig
    pattern 10
pt

adsr
env
    envelope
        section name attack
            ut  115   ul  1.0
            lt  115   ll  1.0
        section name decay
            ut  15  ul  0.9
            lt  15  ll  0.9
        section name decay
            ut  135 ul  0.75
            lt  135 ll  0.75
        section name release
            ut  425 ul  0.0
            lt  425 ll  0.0
    envelope
    in_note_on_trig     pt out_trig
    in_note_off_trig    off
    in_velocity         off
    up_thresh       1.0
    lo_thresh       0.0
    start_level     0.0
    min_time         125.0
    max_sustain_time 0.0
    release_is_ratio off
    sustain_state   off
    zero_retrigger  off
env

clock
cc
    in_freq_mod1 off
    freq 16
    freq_mod1_size 1
cc

inspector
i
    _in_double_float_       env out_output
    _in_double_float_trig_  cc out_phase_trig
    _in_short_int_          off
    _in_short_int_trig_     off
    _in_unsigned_long_      off
    _in_unsigned_long_trig_ off
    _in_state_              off
    _in_state_trig_         off
    _in_string_             off
    _in_string_trig_        off
    _double_float_msg_  sw:out_output=
    _short_int_msg_     .
    _long_int_msg_      .
    _state_msg_         .
    _string_msg_        .
i

wcnt_exit
exit
    in_bar      t out_count
    exit_bar    1
exit

